{
  "network": "livenet",
  "port": 3001,
  "services": [
    "bitcoind",
    "insight-api",
    "web",
    "insight-ui"
  ],
  "servicesConfig": {
    "bitcoind": {
      "spawn": {
        "datadir": "./data",
        "exec": "/usr/local/nvm/versions/node/v4.4.7/lib/node_modules/bitcore-node/bin/bitcoind -maxconnections=50"
      }
    },
    "insight-api": {
      "routePrefix": "api",
      "enableCache": true,
      "disableRateLimiter": true
    },
    "insight-ui": {
      "apiPrefix": "api",
      "routePrefix": ""
    }
  }
}